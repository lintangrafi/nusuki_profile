{
  "version": 2,
  "functions": {
    "public/index.php": {
      "runtime": "@vercel/php"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://nusuki-profile.vercel.app",
    "SESSION_DRIVER": "database",
    "CACHE_DRIVER": "database",
    "QUEUE_CONNECTION": "database"
  }
}