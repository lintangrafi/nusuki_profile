{
  "version": 2,
  "name": "nusuki-profile",
  "builds": [
    {
      "src": "public/vercel-server.php",
      "use": "@vercel/php@9.5.0",
      "config": {
        "composerVersion": "latest",
        "directory": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/vercel-server.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://nusuki-profile.vercel.app",
    "SESSION_DRIVER": "database",
    "CACHE_DRIVER": "database",
    "QUEUE_CONNECTION": "database"
  }
}